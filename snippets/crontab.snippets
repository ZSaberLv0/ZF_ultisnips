snippet cron "crontab expand"
	${0}# minute [0-59]
	# hour [0-23]
	# day of month [1-31]
	# month [1-12]
	# day of week [0-6]
	# user command
	${1:59} ${2:0} ${3:*} ${4:*} ${5:*} ${6:command}
